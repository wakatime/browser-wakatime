{
  "compilerOptions": {
    "esModuleInterop": true,
    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "outDir": "lib",
    "lib": ["es2015", "es2016", "es2017", "dom"],
    "baseUrl": "./",
    "paths": {}
  },
  "exclude": []
}
